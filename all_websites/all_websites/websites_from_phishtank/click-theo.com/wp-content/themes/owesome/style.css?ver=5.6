/*Image Alignment*/

.alignright{
	display: inline;
	float: right;
	margin-left: 1.5em;
}

.alignleft{
	display: inline;
	float: left;
	margin-right: 1.5em;
}

img.alignright{
	float: right;
	margin-left: 1.5em;
}

img.alignleft{
	float: left;
	margin-right: 1.5em;
}

figure.alignleft{
	float: left;
	margin-right: 1.5em;
}

figure.alignright{
	float: right;
	margin-left: 1.5em;
}

figure.aligncenter img[class*="wp-image-"]{
	display: inline-block;
}

.clearfix{
	background-color: transparent;
}
h1.archive_title{
	margin-top: 0px;
}