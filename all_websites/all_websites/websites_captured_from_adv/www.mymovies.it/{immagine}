<script type="text/javascript" charset="utf-8">
	function getCookie(nome) {
	  var nome = nome + "=";
	  var cookies = document.cookie.split(';');

	  for(var i = 0; i < cookies.length; i++) {
	    var c = cookies[i].trim();

	    if (c.indexOf(nome) == 0) 
	      return c.substring(nome.length, c.length);  
	  }
	}
</script>
<div id="norm_cookies" class="img-rounded" style="top:0px; display:none; width:50%; max-width:800px; min-width:250px; background-color:#191919; color:#ffffff; font-size:12px; text-align:center; margin-bottom:2px; position:fixed; border-bottom:2px solid #ffffff; margin-left:25%; margin-right:25%;">
	<div style="width:90%; margin:auto; padding:3px;">
