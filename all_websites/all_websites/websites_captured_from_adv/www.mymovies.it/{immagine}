<script type="text/javascript" charset="utf-8">
	var mymovies_norm_cookie = getCookie('mymovies_norm_cookie');
	var mymovies_mnz_cookie = getCookie('mymovies_mnz_cookie');

	if (mymovies_norm_cookie != mymovies_mnz_cookie) {
		document.cookie='mymovies_mnz_cookie=' + mymovies_norm_cookie + ';expires=Sun, 05 Jan 2050 20:00:00 UTC;path=/';
	}

	if (mymovies_norm_cookie == '1') {
		document.getElementById('norm_cookies').style.display = 'none';
	} else {
		document.getElementById('norm_cookies').style.display = 'block';
	}
	if (mymovies_norm_cookie != '1') {
		window.onscroll = function() {acceptCookie()};
		function acceptCookie() {
			document.cookie='mymovies_norm_cookie=1;expires=Sun, 05 Jan 2050 20:00:00 UTC;path=/';
			document.cookie='mymovies_mnz_cookie=1;expires=Sun, 05 Jan 2050 20:00:00 UTC;path=/';
			document.getElementById('norm_cookies').style.display = 'none';
		}
	}

</script>

