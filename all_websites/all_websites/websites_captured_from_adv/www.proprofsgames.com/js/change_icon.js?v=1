 var icons={

            settingMouseOver:function(){
                document.querySelector("#settingButton a .settingButton").setAttribute("class","settingButton-hover");                      
                                       },
            settingMouseOverOut:function(){                          
               document.querySelector("#settingButton a .settingButton-hover").setAttribute("class","settingButton");

            },
            shareMouseOver:function(){                          
                document.querySelector("#shareButton a .shareButton").setAttribute("class","shareButton-hover");

            },
            shareMouseOverOut:function(){
               document.querySelector("#shareButton a .shareButton-hover").setAttribute("class","shareButton");

            },
            reportMouseOver:function(){
                document.querySelector("#reportButton a .reportButton").setAttribute("class","reportButton-hover");

            },
            reportMouseOverOut:function(){  
               document.querySelector("#reportButton a .reportButton-hover").setAttribute("class","reportButton");

            },
            previewMouseOver:function(){                           
                document.querySelector("#previewButton1 a .previewButton").setAttribute("class","previewButton-hover");

            },
            previewMouseOverOut:function(){                        

               document.querySelector("#previewButton1 a .previewButton-hover").setAttribute("class","previewButton");
            },
            editMouseOver:function(){ 

                edit=document.querySelectorAll(".icon .editIcon");
                for(i=0;i<edit.length;i++)
                {
                    edit[i].setAttribute("class","editIcon-hover");
                }

            },
            editMouseOverOut:function(){                        

               editOver=document.querySelectorAll(".icon .editIcon-hover");
               for(i=0;i<editOver.length;i++)
                {
                    editOver[i].setAttribute("class","editIcon");
                }
            },
            copyMouseOver:function(){ 

                copy=document.querySelectorAll(".icon .copyIcon");
