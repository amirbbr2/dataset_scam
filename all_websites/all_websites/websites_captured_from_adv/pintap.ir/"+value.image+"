            "extensions": [
                "theme-white",
                "position-back",
                "position-right",
                "pagedim-black",
            ],
        },{
            classNames: {
                fixedElements: {
                    fixed: "header-mobile-fix"
                }
            }
    });
    
    $("a[href=#mm-menu]").on("click",function(){
        mmenu.open();
    });
}


