    <script>
        var environment = 'production';
        var ext = {browser: 'Chrome'};
        var advancedScenario = null;
        const locale = 'it';
        const forceInstall = false;

        function appendFontBodyClass() {
            var raf = requestAnimationFrame || mozRequestAnimationFrame ||
                webkitRequestAnimationFrame || msRequestAnimationFrame;
            if (raf) raf(function () {
                window.setTimeout(function () {
                    document.body.classList.add('fonts-loaded')
                }, 0);
            });
            else window.addEventListener('load', function () {
                document.body.classList.add('fonts-loaded')
            });
        }

        function checkFontLoaded() {
            try {
                if (document.fonts.check('1em Roboto')) {
                    appendFontBodyClass();
                }
            } catch (err) {
                appendFontBodyClass();
            }
        }

        document.fonts.ready.then(checkFontLoaded);
        document.fonts.onloadingdone = checkFontLoaded;
        // Fallback timeout
        setTimeout(function () {
            appendFontBodyClass();
        }, 5000)
    </script>

